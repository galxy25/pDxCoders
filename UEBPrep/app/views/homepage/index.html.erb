<div id="home-header">
  <img id="home-logo" src="assets/homepage/logo2.png" alt="image">
</div>
<div id="home-content">
  <h1>Unified English Braille eLearning Platform</h1>
  <table id="home-userInputOuter">
    <tr align="center">
      <td>
        <table id="home-userInputInner">
          <tr align="center">
            <td><%= link_to "Sign Up!", {controller: "users", action: "new"}, class: "home-userInput" %></td>
            <td><%= link_to "Login", {controller: "sessions", action: "create"}, class: "home-userInput" %></td>

          </tr>
          <tr align="center">
            <td colspan="2"><a href="http://www.facebook.com"><img id="facebook-login" src="assets/homepage/FacebookButton.png" alt="image"></td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
  <%= form_tag("homepage/subscribe_email", method: "post", id: "home-subscribe") do %>
    <%= email_field_tag(:email_address) %>
    <p><%= submit_tag("Subscribe", :id=>"home-submit") %></p>
  <% end %>
</div>
