<% provide(:title, "UEBPrep - " + @playlist.name +  " Playlist") %>
<% content_for :navlinks do %>
  <li class="active"><%= link_to 'Edit', edit_playlist_path(@playlist) %></li>
  <li class="active"><%= link_to 'My Playlists', playlists_path %></li>

  <li class="active"><%= link_to 'Next', edit_playlist_path(@playlist) %></li>
  <li class="active"><%= link_to 'Back', playlists_path %></li>

<% end %>
<div class="row app-txt-centered user-card-height">
  <div class="row">
    <div class="small-11 small-centered columns">
      <h1><%= @playlist.name %></h1>
    </div>
  </div>
  <%= render partial: @current_card.content, locals: { content: @current_card.content } %>
</div>
